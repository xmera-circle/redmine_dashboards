$('#errorExplanation').remove();

<% @block_ids.each do |block_id| %>
  $("#block-<%= block_id %>").replaceWith("<%= escape_javascript render_dashboard_block(block_id.to_s, @dashboard) %>");
<% end %>

<% if @dashboard.valid? %>
  reloadOnCurrentPosition();
<% else %>
  $('#content').prepend('<%= escape_javascript(error_messages_for(@dashboard)) %>');
<% end %>

$('[title]').tooltip({
    show: {
      delay: 400
    },
    position: {
      my: "center bottom-5",
      at: "center top"
    }
});
